page{
 height: 100%;
}
.user-avatar>image{
  height: 200rpx;
  width: 300rpx;
}
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
}
.start{
  width: 70%;
  font-size: 40rpx;
  text-align: center;
  height: 50px;
  line-height: 50px;
  border: 1px solid #d6d4d6;
  border-radius: 5px;
  box-shadow: 5px 2px 20px 0 rgba(46, 61, 73, 0.5);
  letter-spacing: 15rpx;
  margin-bottom: 30rpx;
}
.background{
 
  width: 100%;
  height: 100%;
  background-size: cover;
  position: absolute;
  top: 0;
  z-index: -1;

}